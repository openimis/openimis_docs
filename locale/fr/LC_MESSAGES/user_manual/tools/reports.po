msgid "Contribution Distribution Report."
msgstr "Rapport de distribution des contributions."


msgid "`Contribution Distribution Report Criteria`"
msgstr "`Critères de rapport de distribution des contributions`"


msgid "User Activity Report."
msgstr "Rapport d'activité de l'utilisateur."


msgid "`User Activity Report Criteria`"
msgstr "`Critères de rapport d'activité des utilisateurs`"


msgid "Enrolment Performance Indicator Report."
msgstr "Rapport sur les indicateurs de rendement des inscriptions."


msgid "`Enrolment Performance Indicators Report Criteria`"
msgstr "`Critères de rapport des indicateurs de performance des inscriptions`"


msgid "Status of Registers Report."
msgstr "Rapport sur l'état des registres."


msgid "`Status of Registers Report Criteria`"
msgstr "`Critères de rapport sur l'état des registres`"


msgid "Insurees without Photos Report."
msgstr "Assurés sans rapport de photos."


msgid "`Insurees without photos Report Criteria`"
msgstr "`Assurés sans photo Critères de rapport`"


msgid "Payment Category Overview Report."
msgstr "Rapport de présentation des catégories de paiement"


msgid "`Payment Category Overview Report Criteria`"
